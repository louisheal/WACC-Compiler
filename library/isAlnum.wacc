#library function that takes a char and returns true if it is alphanumeric, false otherwise

begin
  bool isAlnum(char c) is
    int x = ord(c);
    if ((x >= 65 && x <= 90) || (x >= 97 && x <= 122) || (x >= 48 && x <= 57))
    then
      return true
    else
      return false
    fi 
  end
  skip
end